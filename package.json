{
  "name": "chronsemble",
  "version": "1.0.0",
  "description": "Multi-mode, co-ordinated, spatio-temporal visualization application.",
  "main": "index.html",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "postinstall": "npm run build:css",
    "build:css": "sass node_modules/d3-geomap/src/sass/geomap.sass > css/geomap.css",
    "clean": "rm -rf node_modules css/geomap.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LAS-NCSU/chronsemble.git"
  },
  "keywords": [
    "timeline",
    "map",
    "info-flow"
  ],
  "author": "John Harkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LAS-NCSU/chronsemble/issues"
  },
  "homepage": "https://github.com/LAS-NCSU/chronsemble#readme",
  "dependencies": {
    "d3": "^3.5.17",
    "d3-geomap": "^1.0.2",
    "patternfly": "^3.41.6"
  }
}
